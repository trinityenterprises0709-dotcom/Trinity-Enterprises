{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "TrinityEnterprises": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/trinity-enterprises",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "**/*",
                "input": "src/shared",
                "output": "/shared"
              }
            ],
            "styles": [
              "src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "node_modules/aos/dist/aos.css",
              "node_modules/glightbox/dist/css/glightbox.min.css",
              "node_modules/swiper/swiper-bundle.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "node_modules/ngx-toastr/toastr.css"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "node_modules/aos/dist/aos.js",
              "node_modules/imagesloaded/imagesloaded.pkgd.min.js",
              "node_modules/swiper/swiper-bundle.min.js",
              "node_modules/glightbox/dist/js/glightbox.min.js",
              "node_modules/isotope-layout/dist/isotope.pkgd.min.js",
              "node_modules/@srexi/purecounterjs/dist/purecounter_vanilla.js",
              "src/assets/main.js"
            ],
            "server": "src/main.server.ts",
            "outputMode": "server",
            "ssr": {
              "entry": "src/server.ts"
            },
            "allowedCommonJsDependencies": [
              "glightbox",
              "isotope-layout",
              "outlayer",
              "get-size",
              "desandro-matches-selector",
              "fizzy-ui-utils",
              "masonry-layout"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "outputHashing": "all",
              "serviceWorker": "ngsw-config.json"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "TrinityEnterprises:build:production"
            },
            "development": {
              "buildTarget": "TrinityEnterprises:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "**/*",
                "input": "src/shared",
                "output": "/shared"
              },
              {
                "glob": "manifest.webmanifest",
                "input": ".",
                "output": "."
              }
            ],
            "styles": [
              "src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "node_modules/aos/dist/aos.css",
              "node_modules/glightbox/dist/css/glightbox.min.css",
              "node_modules/swiper/swiper-bundle.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "node_modules/ngx-toastr/toastr.css"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "node_modules/aos/dist/aos.js",
              "node_modules/imagesloaded/imagesloaded.pkgd.min.js",
              "node_modules/swiper/swiper-bundle.min.js",
              "node_modules/glightbox/dist/js/glightbox.min.js",
              "node_modules/isotope-layout/dist/isotope.pkgd.min.js",
              "node_modules/@srexi/purecounterjs/dist/purecounter_vanilla.js",
              "src/assets/main.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "67b2a4e5-eca7-476d-8be2-6b4dd18d48a0"
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "css",
      "prefix": "app",
      "changeDetection": "OnPush"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    },
    "@schematics/angular:service": {},
    "@schematics/angular:guard": {},
    "@schematics/angular:interceptor": {},
    "@schematics/angular:module": {
      "routing": true
    },
    "@schematics/angular:pipe": {},
    "@schematics/angular:resolver": {}
  }
}